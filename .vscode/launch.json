    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Unit Test",
            "type": "python",
            "request": "test",
            "justMyCode": false,
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "cwd": "${fileDirname}",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "autoReload": {
                "enable": true
            }
        },
        {
            "name": "Python: Current File, cwd = workspace root folder",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "program": "${file}",
            "console": "integratedTerminal",
            "autoReload": {
                "enable": true
            }
        },
        {
            "name": "farn test sample and generate test_farnDict",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}\\tests",
            "program": "${workspaceFolder}\\src\\farn\\cli\\farn.py",
            "args": [
                "test_farnDict",
                "--sample",
                "-g"
            ],
            "console": "integratedTerminal",
            "autoReload": {
                "enable": true
            }
        },
        {
            "name": "farn test generate test_farnDict",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}\\tests\\test_dicts",
            "program": "${workspaceFolder}\\src\\farn\\cli\\farn.py",
            "args": [
                "test_farnDict",
                "-g",
            ],
            "console": "integratedTerminal",
            "autoReload": {
                "enable": true
            }
        },
        {
            "name": "farn test execute test_farnDict",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}\\tests",
            "program": "${workspaceFolder}\\src\\farn\\cli\\farn.py",
            "args": [
                "sampled.test_farnDict",
                "-e",
                "parse",
            ],
            "console": "integratedTerminal",
            "autoReload": {
                "enable": true
            }
        },
        {
            "name": "farn test sample test_farnDict_v4",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}\\tests",
            "program": "${workspaceFolder}\\src\\farn\\cli\\farn.py",
            "args": [
                "test_farnDict_v4",
                "--sample",
            ],
            "console": "integratedTerminal",
            "autoReload": {
                "enable": true
            }
        },
        {
            "name": "farn test generate test_farnDict_v4",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}\\tests",
            "program": "${workspaceFolder}\\src\\farn\\cli\\farn.py",
            "args": [
                "sampled.test_farnDict_v4",
                "-g",
            ],
            "console": "integratedTerminal",
            "autoReload": {
                "enable": true
            }
        },
        {
            "name": "farn test sample and generate test_farnDict_v4",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}\\tests",
            "program": "${workspaceFolder}\\src\\farn\\cli\\farn.py",
            "args": [
                "test_farnDict_v4",
                "--sample",
                "-g"
            ],
            "console": "integratedTerminal",
            "autoReload": {
                "enable": true
            }
        },
        {
            "name": "farn test execute test_farnDict_v4",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}\\tests",
            "program": "${workspaceFolder}\\src\\farn\\cli\\farn.py",
            "args": [
                "sampled.test_farnDict_v4",
                "-e",
                "test",
            ],
            "console": "integratedTerminal",
            "autoReload": {
                "enable": true
            }
        },
        {
            "name": "farn test generate test_farnDict logfile in subdir",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}\\tests",
            "program": "${workspaceFolder}\\src\\farn\\cli\\farn.py",
            "args": [
                "test_farnDict",
                "-g",
                "--log",
                "log\\test.log",
                "--log-level",
                "DEBUG",
            ],
            "console": "integratedTerminal",
            "autoReload": {
                "enable": true
            }
        },
    ]
}